<<<carryplayer:/carryplayer $plr = >>>
    
    @plr = player()
    @vic = $plr

    @ploc = ploc()
    @vicLoc = ploc(@vic)
    
    @players = players_in_radius(@ploc,2)
    
    if(@ploc['world'] != @vicLoc['world']){
        die("Sorry, you have to be in the same world as that person!")
    }

    @x1 = @ploc['x']
    @y1 = @ploc['y']
    @z1 = @ploc['z']

    @x2 = @vicLoc['x']
    @y2 = @vicLoc['y']
    @z2 = @vicLoc['z']

    @x3 = pow(@x1 - @x2, 2)
    @y3 = pow(@y1 - @y2, 2)
    @z3 = pow(@z1 - @z2, 2)

    @final = @x3 + @y3 + @z3

    @distance = sqrt(@final)
   
    if(@distance > 2){
        tmsg(@plr, "Make sure you are close enough to grab that person!")
        die()
    }

    set_entity_rider(puuid(@plr,true),puuid(@vic,true))
    tmsg(@vic, @plr." grabbed you")
    tmsg(@plr, "You grabbed ".@vic."!")
        
<<<

dismoutplayer:/dismountplayer = >>>
   
   @rider = get_entity_rider(puuid(player()))

    if(@rider != null){
        set_entity_rider(null,@rider)
    }

<<<mortal:/mortal = >>>
    @plr = player()
    runas(~console,"/sudo @plr c:/trailsid none")
    runas(~console,"/pex user @plr group set mortal")
    runas(~console,"/warp city @plr")
    
    set_timeout(2000, closure(
        runas(~console,"/gamemode survival @plr")
        runas(~console,"/minecraft:clear @plr")
        runas(~console,"/effect @plr clear")
        runas(~console,"/fly @plr off")
        runas(~console,"/god @plr off")
        runas(~console,"/pex reload")
        runas(~console,"/undisguiseplayer @plr")
    ))
<<< "/effect ". @plr ." 5 9999 6 true",
                "/effect ". @plr ." 13 9999 1 true",
                "/effect ". @plr ." 21 9999 14 true",bind(player_toggle_sneak,null,null,@event){
    @player = player()
    @class = pgroup()[0]
    
    if(has_permission(@player, "powers.wallclimb")){
        @eventID = associative_array('id': @player.'climbwalls')
        @isSneaking = @event['sneaking']
        
        if(@isSneaking){
        
            include('/home/minecraft/server/plugins/CommandHelper/includes/wallClimb.ms')
            @blocksChanged = _spidermanVinePlacement(ploc(),"106",@player)
            export(@player."blocksChanged",@blocksChanged)
        
            @realID = bind(player_move, @eventID, associative_array('player': @player), @event,
            @player, @eventID, @isSneaking){
                
                @blocksChanged = import(@player."blocksChanged")
                foreach(@val in @blocksChanged){
                    psend_block_change(@val,"0:0")
                }

                @blocksChanged = _spidermanVinePlacement(ploc(),"106",@player)
                export(@player."blocksChanged",@blocksChanged)
            }
            export(@eventID['id'],@realID)
        }else{
            unbind(import(@eventID['id']))
            @blocksChanged = import(@player."blocksChanged")
            foreach(@val in @blocksChanged){
                psend_block_change(@val,"0:0")
            }
        }
    
    }
}hoveron:/hover on = >>>
    sudo(/effect player() 25 9999 255 true)
    tmsg(player(),concat(color(gold),
    'You feel lighter than a feather..'))
<<<
hoveroff:/hover off = >>>
    sudo(/effect player() 25 0)
    tmsg(player(),concat(color(gold),
    'Your gravity returns.'))
<<<
